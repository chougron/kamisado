{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["bin/**", "dist", "node_modules"]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "import-name": false,
    "no-console": {
      "severity": "error",
      "options": ["debug", "info", "log", "error"]
    },
    "interface-name": [true, "never-prefix"],
    "prefer-array-literal": false
  }
}
